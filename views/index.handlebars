<h1 class="text-center mb-3">{{title}}</h1>
<form action="/api/members" method="POST" class="mb-4">
<div class="for-group">
    <label for="name">Name</label>
    <input type="text" name="name" class="for-control">
</div>
<div class="for-group">
    <label for="email">Email</label>
    <input type="email" name="email" class="for-control">
</div>
<input type="submit" value="Add Member" class="btn btn-primary btn-block">
</form>
<h4>Members</h4>
<ul class="list-group">
    {{#each members}}
    <li class="list-group-item">{{this.name}} : {{this.email}}</li>
    {{/each}}
</ul>
